<routes>
  <!--Tipo de Automóveis | sigma: drivers’ imperfection in driving (between 0 and 1)-->
  <vType accel="3.0" decel="6.0" id="CarA" color="1,0,0" length="5.0" minGap="2.5" maxSpeed="50.0" sigma="0.2" />
  <vType accel="2.0" decel="6.0" id="CarB" color="0,1,0" length="7.5" minGap="2.5" maxSpeed="50.0" sigma="0.4" />
  <vType accel="1.0" decel="5.0" id="CarC" color="0,0,1" length="5.0" minGap="2.5" maxSpeed="40.0" sigma="0.6" />
  <vType accel="1.0" decel="5.0" id="CarD" color="1,0,1" length="7.5" minGap="2.5" maxSpeed="30.0" sigma="0.8" />
  <!--vType accel="1.0" decel="5.0" id="Car" length="2.0" maxSpeed="100.0" sigma="0.0" /-->
  <!--Tipos de Rotas-->
  <route id="route0" edges="1to0 0to2"/>
  <route id="route1" edges="1to0 0to3"/>
  <route id="route2" edges="2to0 0to1"/>
  <route id="route3" edges="2to0 0to4"/>


  <vehicle depart="1" id="veh0" route="route0" type="CarA" />
  <vehicle depart="10" id="veh1" route="route1" type="CarB" />
  <vehicle depart="20" id="veh2" route="route2" type="CarC" />
  <vehicle depart="30" id="veh3" route="route3" type="CarD" />
</routes>
